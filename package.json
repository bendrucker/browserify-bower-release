{
  "name": "publicist",
  "version": "4.0.1",
  "description": "Generate and tag a UMD build for release on a separate branch",
  "main": "./src",
  "bin": {
    "publicist": "./bin/cli.js"
  },
  "scripts": {
    "test": "standard",
    "build": "babel ./src -d ./lib && babel ./bin -d ./lib-bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bendrucker/publicist.git"
  },
  "keywords": [
    "git",
    "tag",
    "release",
    "bower"
  ],
  "author": "Ben Drucker <bvdrucker@gmail.com> (http://www.bendrucker.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bendrucker/publicist/issues"
  },
  "homepage": "https://github.com/bendrucker/publicist",
  "dependencies": {
    "babel": "~5.2.6",
    "babel-runtime": "~5.1.10",
    "bluebird": "^2.7.1",
    "browserify": "~8.1.1",
    "chalk": "~0.5.1",
    "curry": "~1.2.0",
    "git-assert-clean": "~1.0.0",
    "git-child": "~1.1.0",
    "hat": "0.0.3",
    "packhorse": "~1.1.1",
    "resolve": "~1.1.6",
    "semver": "~4.1.0",
    "yargs": "~3.8.0"
  },
  "devDependencies": {
    "standard": "bendrucker/standard#babel"
  }
}
